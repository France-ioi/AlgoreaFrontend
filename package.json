{
  "name": "algorea",
  "version": "1.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build core && npm run copyColor && npm run copyGeo",
    "extract-i18n": "ng extract-i18n --output-path src/locale && ./node_modules/.bin/xliffmerge",
    "copyColor": "xcopy \"projects\\core\\src\\lib\\colors.scss\" \"dist\\core\\\"",
    "copyGeo": "xcopy \"projects\\core\\src\\lib\\geometry.scss\" \"dist\\core\\\"",
    "test": "ng test",
    "lint": "./node_modules/.bin/eslint src/ --ext .ts",
    "e2e": "ng e2e",
    "release": "release-it"
  },
  "xliffmergeOptions": {
    "srcDir": "src/locale",
    "genDir": "src/locale",
    "defaultLanguage": "en",
    "languages": [
      "fr"
    ],
    "removeUnusedIds": true
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.1.2",
    "@angular/cdk": "^13.1.2",
    "@angular/common": "^13.1.2",
    "@angular/compiler": "~13.1.2",
    "@angular/core": "^13.1.2",
    "@angular/forms": "^13.1.2",
    "@angular/platform-browser": "^13.1.2",
    "@angular/platform-browser-dynamic": "~13.1.2",
    "@angular/router": "^13.1.2",
    "@fortawesome/angular-fontawesome": "^0.10.1",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@ngrx/component": "^13.0.2",
    "fp-ts": "^2.9.5",
    "io-ts": "^2.2.16",
    "jschannel": "^1.0.2",
    "ng-click-outside": "^6.0.0",
    "ngx-perfect-scrollbar": "^8.0.0",
    "primeicons": "^5.0.0",
    "primeng": "^13.1.0",
    "rxjs": "~7.5.1",
    "scss-bundle": "^3.1.1",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.3",
    "@angular-devkit/core": "~13.1.3",
    "@angular-eslint/builder": "^13.0.1",
    "@angular-eslint/eslint-plugin": "^13.0.1",
    "@angular-eslint/eslint-plugin-template": "^13.0.1",
    "@angular-eslint/schematics": "^13.0.1",
    "@angular-eslint/template-parser": "^13.0.1",
    "@angular/cli": "~13.1.3",
    "@angular/compiler-cli": "~13.1.2",
    "@angular/language-service": "~13.1.2",
    "@angular/localize": "^13.1.2",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jschannel": "^1.0.2",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "auto-changelog": "^2.4.0",
    "eslint": "^8.2.0",
    "eslint-plugin-jasmine": "^4.1.2",
    "eslint-plugin-rxjs": "^4.0.4",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^13.1.2",
    "ngx-i18nsupport": "^0.17.1",
    "protractor": "~7.0.0",
    "release-it": "^14.12.4",
    "ts-node": "~7.0.0",
    "typescript": "~4.4.0"
  }
}
